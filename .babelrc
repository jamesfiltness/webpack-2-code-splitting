{
  "env": {
    "node" : {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "react",
        "stage-0"
      ],
      "plugins": ["dynamic-import-node"]
    },
    "browser" : {
      "presets" : [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            }
          }
        ],
        "react",
        "stage-0"
      ]
    }
  }
}
